###############################################################
# Games
###############################################################
###
# create a game
POST http://localhost:8000/games
Content-Type: application/json

{
    "umpire_id": "12345",
    "game_name": "my_game_name",
    "game_password": "secret",
    "maze_id": "maze_id1"
}

###
# get all games
GET http://localhost:8000/games
Content-Type: application/json

{
}
###
# get status of game
GET http://localhost:8000/game/04288a79-a12e-4452-ba45-ffe74fc18a7f/status
Content-Type: application/json

{
}
###
# get map of a game
GET http://localhost:8000/game/04288a79-a12e-4452-ba45-ffe74fc18a7f/map
Content-Type: application/json

{
}
###
# get delta-recording of a game
GET http://localhost:8000/game/04288a79-a12e-4452-ba45-ffe74fc18a7f/recording
Content-Type: application/json

{
}
###
# delete/stop a game
PUT http://localhost:8000/game/04288a79-a12e-4452-ba45-ffe74fc18a7f/stop
Content-Type: application/json

{
}
